{% block formheader %}
    <div class="page-header">
        <h1>Registration</h1>
    </div>
{% endblock %}
    <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
        {{ form_widget(form) }}
        <div>
            <input type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" class="btn-snoreable" />
        </div>
    </form>
